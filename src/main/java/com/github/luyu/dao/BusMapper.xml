<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.luyu.dao.BusMapper">
    <select id="count2" resultType="int">
        select count(0) from bus_attr;
    </select>
    <select id="count1" resultType="int">
        select count(0) from bus_attr where type=1;
    </select>
    <select id="bus" resultType="Bus">
        select * from bus_attr;
    </select>
</mapper>